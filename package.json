{
  "name": "rsbuild-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "nodemon",
    "format": "biome format --write",
    "start": "npm run build && node ./server.mjs",
    "serve": "node ./server.mjs"
  },
  "nodemonConfig": {
    "ignore": [
      "./dist/*",
      "./node_modules/*",
      "./README.md"
    ],
    "ext": "ts,tsx,js,jsx,json,css,scss,md",
    "exec": "NODE_ENV=development npm run start"
  },
  "dependencies": {
    "@mantine/charts": "^7.16.2",
    "@mantine/code-highlight": "^7.16.2",
    "@mantine/core": "^7.16.2",
    "@mantine/dates": "^7.16.2",
    "@mantine/dropzone": "^7.16.2",
    "@mantine/form": "^7.16.2",
    "@mantine/hooks": "^7.16.2",
    "@mantine/modals": "^7.16.2",
    "@mantine/notifications": "^7.16.2",
    "@mantine/nprogress": "^7.16.2",
    "@mantine/spotlight": "^7.16.2",
    "@mantine/tiptap": "^7.16.2",
    "@mjackson/node-fetch-server": "^0.5.1",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@react-router/dev": "^7.1.5",
    "@rsbuild/core": "^1.1.8",
    "@rsbuild/plugin-react": "^1.0.7",
    "@types/express": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.7.2"
  }
}